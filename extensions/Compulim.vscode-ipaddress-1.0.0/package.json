	"version": "1.0.0",
	"publisher": "Compulim",
	"engines": {
		"vscode": "^1.11.0"
	},
	"icon": "icon.png",
	"galleryBanner.color": "#0D5CAB",
	"repository": {
		"type": "git",
		"url": "https://github.com/compulim/vscode-ipaddress.git"
	},
	"bugs": "https://github.com/compulim/vscode-ipaddress/issues/",
