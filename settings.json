        "Vagrantfile": "ruby",
        ".functions": "shellscript",
        ".exports": "shellscript",
        ".vimrc": "shellscript",
        ".extras": "shellscript",
        ".aliases": "shellscript"
    },
    "workbench.colorCustomizations": {
        "activityBarBadge.background": "#2979FF",
        "list.activeSelectionForeground": "#2979FF",
        "list.inactiveSelectionForeground": "#2979FF",
        "button.background": "#2979FF",
        "button.foreground": "#fff",
        "badge.background": "#a5fc03",
        "badge.foreground": "#1e272c",
        "list.highlightForeground": "#2979FF",
        "scrollbarSlider.activeBackground": "#2979FF50",
        "editorSuggestWidget.highlightForeground": "#2979FF",
        "textLink.foreground": "#2979FF",
        "progressBar.background": "#2979FF",
        "pickerGroup.foreground": "#2979FF",
        "tab.activeBorder": "#2979FF",
        "activityBar.background": "#1e272c",
        "sideBar.background": "#1e272c",
        "editorGroupHeader.tabsBackground": "#1e272c",
        "tab.activeBackground": "#2979ff",
        "titleBar.activeBackground": "#2979ff",
        "titleBar.activeForeground": "#fff",
        "tab.inactiveBackground": "#1e272c",
        "statusBar.background": "#1e272c",
        "statusBar.foreground": "#5c7b90",
        "input.background": "#263238",
        "sideBarSectionHeader.background": "#263238",
        "editorWidget.background": "#263238",
        "diffEditor.insertedTextBackground": "#316F01",
    },
    "materialTheme.cache.workbench.settings": {
        "accent": "Blue",
        "accentPrevious": "Orange",
        "themeColours": "One Dark"
    },
    "workbench.iconTheme": "eq-material-theme-icons",
    "editor.smoothScrolling": true,
    "window.zoomLevel": 0,
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressResultsExplorerNotice": true,
        "suppressUpdateNotice": false,
        "suppressWelcomeNotice": true
    },
    "gitlens.blame.line.enabled": false,
    "gitlens.codeLens.enabled": false,
    "files.autoSave": "onFocusChange",
    "phpcs.standard": "psr2",
    "workbench.sideBar.location": "left",
    "editor.tabCompletion": true,
    "diffEditor.renderSideBySide": false,
    "extensions.ignoreRecommendations": false,
    "editor.wordWrap": "on",
    "json.format.enable": false,
    "editor.autoIndent": false,"auto-close-tag.activationOnLanguage": [
